<odoo>
    <record id="ai_agent_form_inherit_custom" model="ir.ui.view">
        <field name="name">ai.agent.form.inherit.custom</field>
        <field name="model">ai.agent</field>
        <field name="inherit_id" ref="ai_app.ai_agent_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group" position="inside">
                <field name="use_external_ai"/>
                <field name="custom_prompt"/>
            </xpath>
        </field>
    </record>
</odoo>
